{
  "name": "lilypad",
  "version": "0.0.1",
  "description": "front-end-infra for large apps in madadata",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev babel-node server.js",
    "start:dev": "nodemon --exec babel-node --presets=es2015 server.js",
    "start:prod": "NODE_ENV=prod babel-node server.js",
    "test": "./node_modules/.bin/karma start ./config/karma.config.js",
    "test:watch": "npm run test -- --watch",
    "build:server": "cross-env NODE_ENV=prod babel server.js -o server-compiled.js",
    "build:webpack": "cross-env NODE_ENV=prod ./node_modules/.bin/webpack --config ./config/webpack.config.prod.js",
    "build": "npm-run-all build:server build:webpack",
    "clean": "rimraf dist server-compiled.js",
    "lint": "eslint server.js src",
    "pretest": "npm run lint",
    "prebuild": "npm-run-all clean lint"
  },
  "author": "Zheng He <zhenghe@madadata.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel": "6.5.2",
    "babel-cli": "6.10.1",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.0.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-loader": "1.4.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "2.0.1",
    "eslint-plugin-react": "5.2.2",
    "eventsource-polyfill": "0.9.6",
    "file-loader": "0.9.0",
    "ignore-styles": "4.0.0",
    "json-loader": "0.5.4",
    "karma": "1.1.1",
    "karma-chai": "0.1.0",
    "karma-mocha": "1.1.1",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.7.0",
    "lost": "7.0.3",
    "mocha": "2.5.3",
    "nodemon": "1.9.2",
    "npm-run-all": "2.3.0",
    "phantomjs-polyfill": "0.0.2",
    "postcss": "5.1.0",
    "postcss-cssnext": "2.7.0",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.9.1",
    "react-addons-test-utils": "15.2.1",
    "redux-logger": "2.6.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.1",
    "yargs": "4.8.0"
  },
  "dependencies": {
    "express": "4.14.0",
    "helmet": "2.1.1",
    "lodash": "4.13.1",
    "morgan": "1.7.0",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.5.2",
    "redux": "3.5.2",
    "redux-saga": "0.11.0",
    "reselect": "2.5.3",
    "serve-favicon": "2.3.0",
    "winston": "2.2.0"
  }
}
